{
  "name": "svelte-effector-tailwind",
  "version": "1.0.0",
  "dependencies": {
    "sirv-cli": "^0.4.4",
    "effector": "^20.6.1",
    "tailwindcss": "^1.1.3",
    "tailwindcss-transitions": "^2.1.0",
    "autoprefixer": "^9.7.1",
    "cross-env": "^6.0.3",
    "cssnano": "^4.1.10",
    "npm-run-all": "^4.1.5",
    "node-sass": "^4.13.0",
    "sass": "^1.23.3",
    "rollup": "^1.27.0",
    "postcss-cli": "^6.1.2",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.1",
    "postcss-custom-properties": "^9.0.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-livereload": "^1.0.4",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.1.1",
    "rollup-plugin-terser": "^5.1.2",
    "svelte": "^3.14.1",
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "svelte-preprocess": "^3.2.6"
  },
  "scripts": {
    "watch:tailwind": "postcss public/*.pcss --ext css --dir public -w",
    "build:tailwind": "NODE_ENV=production postcss public/*.pcss --dir public --ext css",
    "dev": "run-p start:dev autobuild watch:tailwind",
    "build": "npm run build:tailwind && rollup -c",
    "autobuild": "rollup -c -w",
    "start": "npm run build && sirv public --port $PORT --host 0.0.0.0 ",
    "start:dev": "sirv public --dev --port $PORT --host 0.0.0.0 "
  }
}
